<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="CallOutServiceMEXBehavior" name="CallOut_CodingServiceLib.CallOut_CodingService">
        <endpoint address="service" binding="netTcpBinding" bindingConfiguration="customTcpBinding" contract="CallOut_CodingServiceLib.IMessageServiceInbound" />
        <!--<endpoint address="service" binding="wsDualHttpBinding" contract="CallOut_CodingServiceLib.IMessageServiceInbound" />-->
        <endpoint address="mex" binding="mexTcpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:8080/CallOut_CodingService/" />
            <!--<add baseAddress="http://localhost:8080/CallOut_CodingService" />-->
          </baseAddresses>
        </host>
      </service>
    </services>
    <bindings>
      <netTcpBinding>
        <binding name="customTcpBinding">
            <security mode="None">
            <message clientCredentialType="None"/>
            <transport protectionLevel="None" clientCredentialType="None"/>
          </security>
          <reliableSession enabled="false"/>
        </binding>
      </netTcpBinding>
    </bindings>
    <!-- A behavior definition for MEX -->
    <behaviors>
      <serviceBehaviors>
        <behavior name="CallOutServiceMEXBehavior">
          <serviceMetadata httpGetEnabled="false"/>
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
